  "private": true,
  "main": "src/start.js",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.1",
    "@testing-library/user-event": "^7.2.1",
    "cross-env": "^7.0.0",
    "csv-writer": "^1.6.0",
    "electron": "^8.0.2",
    "electron-is-dev": "^1.1.0",
    "moment": "^2.24.0",
    "python-shell": "^1.0.8",
    "react": "^16.13.0",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^16.13.0",
    "react-icons": "^3.9.0",
    "react-moment": "^0.9.7",
    "react-papaparse": "^2.0.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0",
    "react-simple-checkbox": "^1.3.0"
  },
  "build": {
    "appId": "myid",
    "directories": {
      "buildResources": "assets"
    }
  },
  "scripts": {
    "electron": "cross-env NODE_ENV=dev nodemon --exec \"\"electron .\"\"",
    "tailwind:css": "./node_modules/.bin/tailwind build src/css/tailwind.src.css -o src/css/tailwind.css",
    "start": "cross-env BROWSER=none npm run tailwind:css && react-scripts start",
    "build": "npm run tailwind:css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron-builder": "^22.3.2",
    "electron-reload": "^1.5.0",
    "nodemon": "^2.0.2",
    "tailwindcss": "^1.2.0"
  }
