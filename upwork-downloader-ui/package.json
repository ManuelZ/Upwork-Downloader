{
  "name": "upwork-downloader-ui",
  "version": "0.1.0",
  "private": true,
  "main": "src/start.js",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.1",
    "@testing-library/user-event": "^12.2.0",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "electron": "^10.1.5",
    "electron-is-dev": "^1.2.0",
    "history": "^5.0.0",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "react": "^17.0.1",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^17.0.1",
    "react-icons": "^3.11.0",
    "react-moment": "^1.0.0",
    "react-router-dom": "^6.0.0-beta.0",
    "react-scripts": "^4.0.0",
    "react-simple-checkbox": "^1.3.0"
  },
  "build": {
    "appId": "myid",
    "directories": {
      "buildResources": "assets"
    }
  },
  "scripts": {
    "tailwind:css": "./node_modules/.bin/tailwind build src/css/tailwind.src.css -o src/css/tailwind.css",
    "start": "npm run tailwind:css && cross-env BROWSER=none react-scripts start",
    "electron": "cross-env NODE_ENV=dev nodemon --exec \"\"electron .\"\"",
    "both": "concurrently \"npm start\" \"npm run electron\"",
    "build": "npm run tailwind:css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron-builder": "^22.9.1",
    "electron-reload": "^1.5.0",
    "nodemon": "^2.0.6",
    "tailwindcss": "^1.9.6"
  }
}
